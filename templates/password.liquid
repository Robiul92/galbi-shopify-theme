<section class="coming_soon" style="background-image: url({% if settings.password_img != blank %}{{settings.password_img|img_url:'master'}}{% else %}//placehold.it/1920x1080{% endif %})">
  <div class="top_coming_soon">
    <div class="container text-center">
      <span>
      {% assign image_url = 'https://cdn.shopify.com/s/files/1/0756/1412/6358/files/Galbi_Final_Deliverables-01_430x.png?v=1683726398#' %}
<img src="{{ image_url }}" alt="Dynamic Image">
</span>
      {% if settings.password_title != blank %}
      <span class="text-uppercase">{{settings.password_title}}</span>
      {% endif %}
      {% if settings.description_password_title != blank %}
      <h1 class="mb-0">{{settings.description_password_title}}</h1>
      {% endif %}
    </div>
  </div>
  <div class="container-fluid">
    <div class="time">
      <div class="row justify-content-center text-center">
        {% if settings.ymdhps_password != blank %}
        <ul class="mb-0 list-inline list-unstyled " data-countdown="password-countdown" data-date="{{settings.ymdhps_password}}">
        </ul>
        {% endif %}
      </div>
    </div>
  </div>
  <div class="signup">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-6 col-md-6 col-sm-9 text-center">

<!-- Begin Mailchimp Signup Form -->
