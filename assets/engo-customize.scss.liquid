.tab {
  width: 100%;
  color: #000;
  overflow: hidden;
  border-bottom: 2px solid #000;
}
.tab-label {
  display: flex;
  justify-content: space-between;
  padding: 18px 0px 3px 0px;
  cursor: pointer;
  font-family: 'Brooklyn';
  font-size: 12px;
  /* Icon */
}
/* .tab-label:hover {
  background: #1a252f;
} */
.tab-label::after {
  content: "+";
  width: 1em;
  height: 1em;
  text-align: center;
  transition: all 0.35s;
}
.tab-content {
  max-height: 0;
  padding: 0 1em;
  color: #000;
  // background: white;
  transition: all 0.35s;
  text-align: left;
  font-size: 12px;
  font-family: 'Open Sans' !important;
  line-height: 25px;
}
.tab-close {
  display: flex;
  justify-content: flex-end;
  padding: 1em;
  font-size: 0.75em;
  background: #2c3e50;
  cursor: pointer;
}
.tab-close:hover {
  background: #1a252f;
}

/* input:checked + .tab-label {
  background: #1a252f;
} */
input:checked + .tab-label::after {
   content: "-";
}
input:checked ~ .tab-content {
  max-height: 100vh;
  padding: 1em 0;
}


