.list-category{
  padding: 50px 0;
  .img_collection{
    position: relative;
    overflow: hidden;
    img{
    	transition : .3s;
    }
    &:before, &:after{
      content: '';
      position: absolute;
      top: 0;
      bottom: 0;
      right: 0;
      left: 0;
      -webkit-transition: all 0.6s ease;
      -moz-transition: all 0.6s ease;
      -o-transition: all 0.6s ease;
      transition: all 0.6s ease;
      z-index: 1;
    }
    &:hover{
      img{
      	transform: scale(1.05);
        transition : .3s;
      }
      &:before{
        right: 50%;
        left: 50%;
        background-color: rgba(255,255,255,0.5);
      }
      &:after{
        top: 50%;
        bottom: 50%;
        background-color: rgba(255,255,255,0.5);
      }
    }
  }
  .info_col{
    .title_collection{
    	font-size: 30px;
      	font-weight: 300;
      	font-family: $font-primary;
      	color: #000;
      	margin-top: 30px;
    }
    .total_item{
    	font-size: 16px;
      	font-weight: 400;
      	font-family: $font-primary;
      	color: #000;
    }
    .shop-button{
      margin-top:20px;
      border-radius: 0px;
      padding: 18px 50px;
      text-transform: uppercase;
      color:$color-main;
      position: relative;
      display: inline-block;
      transition: all 0.5s ease-out;
      font-size: 14px;
      font-weight: 600;
      letter-spacing:2px;
      border:2px solid $color-main;
      z-index: 1;
      &:after {
        -webkit-transition:0.5s all ease;
        transition:0.5s all ease;
        position:absolute;
        top:0;
        left:50%;
        right:50%;
        bottom:0;
        opacity:0;
        content:'';
        background-color:$color-main2;
        z-index: -1;
      }
      &:hover {
        color:#fff;
        border:2px solid $color-main2;
        &:after {
          -webkit-transition:0.5s all ease;
          transition:0.5s all ease;
          left:0;
          right:0;
          opacity:1;
        }
      }
      &:focus {
        &:after {
          transition:0.5s all ease;
          left:0;
          right:0;
          opacity:1;
        }
      }
    }
  }
}
